{{#partial "contenido"}}
<h1>Pois</h1>
<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Direccion</th>
		<th></th>
	</tr>
	
	{{#each pois}}
		<tr>
			<td>{{nombre}}</td>
			<td>{{direccion}}</td>
			<td><a href="{{url}}">Ver</a><td>
		</tr>
	{{/each}}
</table>
<form action="/terminal/buscarPois" method="GET">
		<input class="btn btn-primary btn-lg" type="submit" value="Volver">
</form>
{{/partial}}

{{>layoutTerminal.hbs}}